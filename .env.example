# AnarQ&Q Ecosystem Demo Orchestrator Environment Configuration

# Environment Selection
NODE_ENV=development
DEMO_ENVIRONMENT=local

# QNET Phase 2 Configuration
QNET_PHASE2_ENABLED=false
QNET_MIN_NODES=3
QNET_CPU_THRESHOLD=70
QNET_MEMORY_THRESHOLD=80
QNET_NETWORK_LATENCY_THRESHOLD=200

# Pi Network Configuration
PI_NETWORK_ENABLED=true
PI_NETWORK_TESTNET=true
PI_GOVERNANCE_CONTRACT=0x1234567890123456789012345678901234567890
PI_DAO_CONTRACT=0x2345678901234567890123456789012345678901

# IPFS Configuration
IPFS_GATEWAY=http://localhost:8080
IPFS_API=http://localhost:5001
IPFS_TIMEOUT=30000

# Performance Validation Gates
MAX_LATENCY=2000
MIN_THROUGHPUT=100
MAX_ERROR_RATE=0.01

# Decentralization Validation Gates
MIN_NODES=3
MAX_SINGLE_POINT_FAILURES=0
MIN_GEOGRAPHIC_DISTRIBUTION=2

# Module Configuration
MODULES_BASE_PORT=3000
MODULES_HOST=localhost

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Security Configuration
ENABLE_QERBEROS_VALIDATION=true
ENABLE_AUDIT_TRAIL_VALIDATION=true
ENABLE_DATA_INTEGRITY_CHECK=true